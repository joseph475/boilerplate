# Backend-specific environment variables
# These will override any variables with the same name from the root .env

# Backend-specific port (if different from root)
# PORT=8001

# Backend-specific database settings
DB_POOL_SIZE=10
DB_TIMEOUT=30000

# Backend-specific logging
LOG_LEVEL=debug
LOG_FILE=backend.log

# Backend-specific rate limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Backend-specific version
APP_VERSION=1.0.0-backend
